<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <PackageTargetFallback>$(PackageTargetFallback);portable-net45+netcore45+wpa81</PackageTargetFallback>
    <Version>7.1.1-alpha</Version>
    <Authors>David Young</Authors>
    <Company />
    <Product>Magnesium</Product>
    <Description>C#/.net bindings of the Vulkan 1.0 library</Description>
    <PackageTags>graphics, vulkan</PackageTags>
    <PackageProjectUrl>https://github.com/tgsstdio/Mg</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/tgsstdio/Mg/blob/master/LICENSE.txt</PackageLicenseUrl>
    <Copyright>Copyright (c) 2016, 2017 David Young</Copyright>
    <RepositoryUrl>https://github.com/tgsstdio/Mg.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>7.1.1.0</AssemblyVersion>
    <FileVersion>7.1.1.0</FileVersion>
    <PackageReleaseNotes>- Migrated library to .net standard 1.1 (equivalent to PCL profile 111)</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputPath>..\bin\Release\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputPath>..\bin\Debug\netstandard1.1\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Magnesium" Version="7.1.0-alpha" />
  </ItemGroup>
</Project>